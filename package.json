{
  "name": "@fdmg/imgix-upload",
  "version": "0.0.9",
  "description": "Command-line tool to upload images to the FDMG IMGIX S3 Bucket",
  "main": "dist/app.js",
  "preferGlobal": true,
  "bin": {
    "fiu": "cli.js",
    "fdmg-imgix-upload": "cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:prod": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "dry-run": "npm run start -- --dry-run -s -v",
    "dogfood": "dotenv fiu -- -v -s -i ./images",
    "dogfood2": "dotenv fiu -- -v -s -i ./public/images",
    "help": "node -r dotenv/config dist/app.js --help",
    "start": "node -r dotenv/config dist/app.js -o ./imageMap.json -i ./images -s",
    "start2": "node -r dotenv/config dist/app.js -e acceptance -o ./imageMap.json -i ./public/images -s",
    "release:fdmg": "node modifyPackageJson.js -s @fdmg",
    "release:fdmediagroep": "node modifyPackageJson.js -s @fdmediagroep",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --mode=development -w",
    "watch:prod": "webpack --mode=production -w"
  },
  "author": "Willem Liu",
  "license": "ISC",
  "devDependencies": {
    "@fdmg/imgix-upload": "^0.0.9",
    "@types/glob": "^7.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/progress": "^2.0.5",
    "@types/yargs": "^17.0.10",
    "assert": "^2.0.0",
    "aws-sdk": "^2.1123.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "edit-json-file": "^1.7.0",
    "file-extension": "^4.0.5",
    "glob": "^8.0.1",
    "mime-types": "^2.1.35",
    "progress": "^2.0.3",
    "sha1-file": "^3.0.0",
    "ts-loader": "^9.2.9",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "yargs": "^17.4.1",
    "zlib": "^1.0.5"
  }
}
